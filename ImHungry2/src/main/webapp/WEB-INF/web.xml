<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>YelpResults</servlet-name>
  	<display-name>YelpResults</display-name>
  	<description></description>
  	<servlet-class>yelp.YelpResults</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>restaurantData</servlet-name>
  	<display-name>restaurantData</display-name>
  	<description></description>
  	<servlet-class>yelp.restaurantData</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>YelpResults</servlet-name>
  	<url-pattern>/YelpResults</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>restaurantData</servlet-name>
  	<url-pattern>/restaurantData</url-pattern>
  </servlet-mapping>
</web-app>
